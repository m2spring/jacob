                       ,/* typeRepr        =: */ _
                       ,/* complete        =: */ complete
                       ,/* position        =: */ position
                       ,/* label           =: */ ..)
,/* currPosition =: */ ..
                       RETURN table ?
                       { IF ~complete THEN 
                            fExportMode := E.MaxExportMode(fExportMode,eExportMode);
                            position    := currPosition;
